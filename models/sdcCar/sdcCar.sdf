            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>0</lower>
                    <upper>0</upper>
                </limit>
            </axis>
            <axis2>
                <xyz>0 1 0</xyz>
            </axis2>
            <physics>
                <ode>
                    <limit>
                        <cfm>0.0</cfm>
                        <erp>0.9</erp>
                    </limit>
                    <suspension>
                        <cfm>0.01</cfm>
