                <cast_shadows>1</cast_shadows>
                <transparency>0</transparency>
              </visual>
        </link>


        <!-- JOINT BETWEEN CAMERA SENSOR AND CAR -->
        <joint name='camera' type='revolute'>
          <parent>chassis</parent>
          <child>camera::link</child>
          <axis>
            <xyz>0 0 1</xyz>
            <use_parent_model_frame>0</use_parent_model_frame>
            <limit>
              <lower>0</lower>
              <upper>0</upper>
              <effort>-1</effort>
              <velocity>-1</velocity>
            </limit>
            <dynamics>
              <spring_reference>0</spring_reference>
              <spring_stiffness>0</spring_stiffness>
              <damping>0</damping>
              <friction>0</friction>
            </dynamics>
          </axis>
          <physics>
            <ode>
              <limit>
                <cfm>0</cfm>
                <erp>0.2</erp>
              </limit>
              <suspension>
                <cfm>0</cfm>
                <erp>0.2</erp>
              </suspension>
            </ode>
          </physics>
        </joint>

        <!-- GPS SENSOR -->
        <link name="gps">
            <pose>0.0 0.0 1.0 0 0 0</pose>
            <visual name="visual">
                <geometry>
                    <mesh>
                        <uri>model://hokuyo/meshes/hokuyo.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            <collision name="gps_collision-base">
                <pose>0 0 -0.0145 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.041</size>
                    </box>
                </geometry>
            </collision>
            <collision name="gps_collision-top">
                <pose>0 0 0.0205 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.021</radius>
                        <length>0.029</length>
                    </cylinder>
                </geometry>
            </collision>
            <sensor name="gps" type="gps">
                <pose>0.0 0 0.0 0 0 0</pose>
                <!-- <plugin name="sdcGpsSensor" filename="build/libsdcGpsSensor.so" /> -->
                <always_on>1</always_on>
                <update_rate>30</update_rate>
            </sensor>
        </link>

        <!-- JOINT BETWEEN GPS SENSOR AND CAR -->
        <joint name="gps_joint" type="revolute">
            <child>gps</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <upper>0</upper>
                    <lower>0</lower>
                </limit>
            </axis>
        </joint>


        <!-- PLUGINS FOR THE CAR; ESSENTIALLY THE "BRAINS" OF THE CAR -->
        <plugin filename="build/libsdcCar.so" name="sdcCar">
            <!-- Joints for each wheel -->
            <front_left>front_left_joint</front_left>
            <front_right>front_right_joint</front_right>
            <back_left>back_left_joint</back_left>
            <back_right>back_right_joint</back_right>
            <!-- This is the chassis -->
            <chassis>chassis</chassis>
            <!-- Radius of the wheel (DO NOT CHANGE UNLESS YOU ALSO CHANGE THE ACTUAL PARAMETER ABOVE PLS) -->
            <wheel_radius>0.21</wheel_radius>
            <!-- Power is multiplied by "gas" to determine force applied to wheels -->
            <front_power>10</front_power>
            <rear_power>10</rear_power>
            <!-- The angle range the front tires can turn for steering, in radians -->
            <tire_angle_range>0.5</tire_angle_range>
            <!-- Maximum speed for the car in meters/second -->
            <max_speed>10</max_speed>
            <!-- Factor for the down-force applied as the car travels faster -->
            <!-- A larger number increases the down force -->
            <aero_load>1.0</aero_load>
        </plugin>

        <plugin name="sdcGpsSensor" filename="build/libsdcGpsSensor.so">
            <gps>gps</gps>
        </plugin>
    </model>
</sdf>
